---
import "../styles/docs.css";
import GetDocs from "../components/GetDocs.astro";
import BaseLayout from "../layouts/BaseLayout.astro";
const pageTitle = "笔记";
const pageDescription = "积跬步以至千里";
---

<style is:global>

    .aside-links a.iconfont.icon-pc {
        color: #ff6100 !important;
        background-color: #007175 !important;
    }
    
    .article-container p{
        text-indent: 0;
    }
    
</style>
    
<BaseLayout pageTitle={pageTitle} pageDescription={pageDescription}>
    <GetDocs />
</BaseLayout>
